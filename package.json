{
    "name": "mine-sweeper",
    "version": "1.0.0",
    "description": "classic MineSweeper game",
    "main": "index.js",
    "config": {
        "distDirectory": "dist"
    },
    "scripts": {
        "clean": "rm -rf dist",
        "test": "nyc mocha --compilers js:babel-core/register  --recursive",
        "build": "grunt copy && node devServer.js",
        "sdev": "node devServer.js",
        "start": "set NODE_ENV=production && node devServer.js",
        "lint": "eslint src/**",
        "lint:fix": "eslint src/**  --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bamorgans/mine-sweeper.git"
    },
    "keywords": [
        "minesweeper",
        "ReactJS",
        "Redux"
    ],
    "author": "Bob Morganstern",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/bamorgans/mine-sweeper/issues"
    },
    "homepage": "https://github.com/bamorgans/mine-sweeper#readme",
    "dependencies": {
        "express": "^4.16.2",
        "lodash": "^4.17.4",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.1",
        "babel-loader": "^7.1.2",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "babel-register": "^6.26.0",
        "eslint": "^4.16.0",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-react": "^7.6.1",
        "grunt": "^1.0.1",
        "grunt-contrib-copy": "^1.0.0",
        "mocha": "^5.0.0",
        "nyc": "^11.4.1",
        "react-hot-loader": "^3.1.3",
        "redux": "^3.7.2",
        "uglifyjs-webpack-plugin": "^1.1.8",
        "webpack": "^3.10.0",
        "webpack-dev-middleware": "^2.0.5",
        "webpack-hot-middleware": "^2.21.0"
    }
